service touchupdate /system/bin/vts_app --update
	class core
	user system
	disabled
	seclabel u:r:vts_app:s0
	oneshot

service touchtest /system/bin/vts_app --sensor-test
	class core
	user system
	disabled
	seclabel u:r:vts_app:s0
	oneshot
